{
  "version": 2,
  "builds": [
    {
      "src": "stock_signal/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "45mb",
        "runtime": "python3.9"
      }
    },
    {
      "src": "staticfiles/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "stock_signal/wsgi.py"
    }
  ]
}
